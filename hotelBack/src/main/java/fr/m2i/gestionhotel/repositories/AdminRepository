package fr.m2i.gestionhotel.repositories;

import fr.m2i.gestionhotel.entities.AdminEntity;
import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.data.repository.CrudRepository;

import java.util.List;

public interface AdminRepository extends JpaRepository<AdminEntity, Integer> {

    public AdminEntity findByUsername(String username);

    public AdminEntity findByUsernameContains(String username);

    public AdminEntity findByUsernameContainsOrRole(String username, String role);

}